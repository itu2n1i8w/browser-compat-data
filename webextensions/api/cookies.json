              },
              "firefox": {
                "notes": [
                  "Provides access to cookies from private browsing mode and container tabs since version 52."
                ],
                "version_added": "45"
              },
              "firefox_android": {
                "version_added": "48"
              },
              "opera": {
                "version_added": true
              }
            }
